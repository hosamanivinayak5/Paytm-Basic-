{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods":["GET","POST","PUT","POST","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ]
}